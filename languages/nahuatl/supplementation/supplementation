
(df supplementation
	(constituent nc verbal intransitive)
	(into (theta subject) constituent adjunct))

(for poss (partitive owner)
(df supplementation
	(theta (poss .poss))
	(seme -.poss)
	(into (theta pers) constituent adjunct)
	(constituent nc nominal absolutive)))

(df supplementation
	(into (theta pers) constituent adjunct)
	(into (theta poss) constituent adjunct)
	(constituent nc nominal possessive))

; VSO order is extremely common.
(df supplementation
	(constituent nc verbal transitive)
	(into (theta subject) constituent adjunct)
	(into (theta  object) constituent adjunct))

; VOS order is also OK.
(df supplementation
	(constituent nc verbal transitive)
	(into (theta subject) constituent adjunct)
	(into (theta  object) constituent adjunct))


