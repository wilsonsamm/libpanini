; The Pronominal NNC is described in Lesson 16 of Introduction to Classical Nahuatl

(df pronominal-stem
	(flags first second)
	(segments e saltillo))

(df absolutive
	(flags singular)
	(seme (number singular))
	(segments tl))
	
(df absolutive
	(flags plural)
	(seme (number plural))
	(segments t i n))

(df absolutive
	(flags animate)
	(seme (number singular))
	(segments i n))

(df nc
	(sandhi -suppress-whitespace)
	(flags nominal pronominal)
	(constituent personal-affix realis)
	(constituent pronominal-stem singular)
	(segments #))

(df nc
	(sandhi -suppress-whitespace)
	(flags nominal pronominal)
	(constituent personal-affix realis)
	(constituent pronominal-stem)
	(segments w aa)
	(constituent absolutive singular)
	(rection (number singular))
	(segments #))

(df nc
	(sandhi -suppress-whitespace)
	(flags nominal pronominal)
	(constituent personal-affix realis)
	(constituent pronominal-stem)
	(segments w aa n)
	(constituent absolutive plural)
	(rection (number plural))
	(segments #))
	
