; The Pronominal NNC is described in Lesson 16 of Introduction to Classical Nahuatl
; This is an incomplete implementation of some of those.

(df pronominal-stem
	(flags first second)
	(segments e saltillo))

(df nc
	(sandhi -suppress-whitespace)
	(flags nominal pronominal)
	(constituent personal-affix realis)
	(constituent pronominal-stem)
	(segments w aa)
	(constituent absolutive tl)
	(rection (number singular))
	(segments #))

(df nc
	(sandhi -suppress-whitespace)
	(flags nominal pronominal)
	(constituent personal-affix realis)
	(constituent pronominal-stem)
	(segments w aa n)
	(constituent absolutive tin)
	(rection (number plural))
	(segments #))
	
