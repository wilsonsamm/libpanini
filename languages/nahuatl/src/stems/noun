(df nounstem
	(flags nominal in)
	(segments w ee y m i ch )
	(seme (head whale))
	(theta (poss owner)))

(df nounstem
	(flags in tli nominal)
	(segments t oo ch)
	(seme (head rabbit))
	(theta (poss owner)))

; How to learn a nounstem
(df nounstem
	(flags .gender .animacy)
	(open verbstem
		(segments 6)
		(at-least-one-segment)
		(flags .gender .animacy)
		(seme head)))
