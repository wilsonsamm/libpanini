(df verb
	(flags intransitive)
	(rection -object)
	(constituent vprefix subject)
	(constituent tense-marker)
	(constituent verbstem intransitive)
	(space)
	(clues (class verb)))

(df verb
	(flags transitive)
	(rection object)
	(constituent vprefix subject)
	(constituent tense-marker)
	(constituent vprefix object)
	(constituent verbstem transitive)
	(space)
	(clues (class verb)))
	
(df verb
	(flags copula)
	(lit ni) (space)
	(seme -negative))

;(df verb
;	(flags copula)
;	(lit si) (space)
;	(seme negative))
