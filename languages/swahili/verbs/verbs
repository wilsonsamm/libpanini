(df verb
	(flags intransitive)
	(rection -object)
	(into (theta subject) constituent prefix subject)
	(constituent tense-marker)
	(constituent verbstem intransitive)
	(space)
	(clues (class verb)))

(df verb
	(flags transitive)
	(rection object)
	(into (theta subject) constituent prefix subject)
	(constituent tense-marker)
	(into (theta object)  constituent prefix object)
	(constituent verbstem transitive)
	(space)
	(clues (class verb)))


(df verb
	(flags copula)
	(lit ni) (space)
	(seme -negative))

(df verb
	(flags copula)
	(lit si) (space)
	(seme negative))
