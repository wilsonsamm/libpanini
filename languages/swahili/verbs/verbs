(df verb
	(flags intransitive)
	(rection -object)
	(into (theta subject) constituent prefix subject)
	(constituent tense-marker)
	(constituent verbstem intransitive)
	(space))

(df verb
	(flags transitive)
	(rection object)
	(into (theta subject) constituent prefix subject)
	(constituent tense-marker)
	(into (theta object)  constituent prefix object)
	(constituent verbstem transitive)
	(space))
