(df noun
	(flags m/wa singular)
	(rection (class m/wa) (number singular))
	(segments m w)
	(call nounstem m/wa)
	(space)
	(seme (number singular))
	(clues (class nominal)))

(df noun
	(flags m/wa plural)
	(segments w a)
	(rection (class m/wa) (number plural))
	(call nounstem m/wa)
	(space)
	(seme (number plural))
	(clues (class nominal)))

; To add a M/WA-class noun to the dictionary, include these flags:
;    m/wa    - this is M/WA class nounstem

(df nounstem
	(flags m/wa)
	(segments t u)
	(fork
		((seme (head person)))
		((seme (head man)))))

(df nounstem
	(flags m/wa)
	(segments t o t o)
	(seme (head child)))

(df nounstem
	(flags m/wa)
	(segments a n a)
	(seme (head offspring)))
	
(df nounstem
	(flags m/wa)
	(segments j o m b a)
	(seme (head uncle) (familial-side mother)))
