; These prefixes reduce the valency of the verb by one.

(df operation
	(flags valency-reduction)
	(lit yay)
	(seme (trajectory reflexive)))

(df operation
	(flags valency-reduction)
	(lit u)
	(seme (trajectory reciprocative)))
	
(for theta (patient agent)
(df operation
	(flags valency-reduction)
	(lit i)
	(theta (patient .theta)) (seme -.theta)))
