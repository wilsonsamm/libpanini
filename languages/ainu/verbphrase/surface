(df verb-and-affices
	(constituent personal-prefix monovalent)
	(constituent verbstem active))

(df verb-and-affices
	(constituent verbstem active)
	(constituent personal-suffix monovalent))

(df verb-and-affices
	(constituent personal-prefix monovalent)
	(into (head) constituent verbstem stative))

(df verb-and-affices
	(into (head) constituent verbstem stative)
	(constituent personal-suffix monovalent))

(df verb-and-affices
	(constituent personal-prefix bivalent)
	(constituent verbstem bivalent))
	


(df verb
	(constituent verb-and-affices)
	(space))
