
;(for valency (avalent monovalent bivalent copula trivalent))
(for valency (monovalent bivalent)
	(df verb
		(flags .valency)
		(debug)
		(constituent personal-prefix .valency)
		(constituent personal-stem .valency)))
	
(df verb
	(constituent personal-stem monovalent)
	(constituent personal-suffix))
