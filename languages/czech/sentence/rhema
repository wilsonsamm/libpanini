;; First position in the rhema
; (likely to be a verb)
(df rhema
	(flags 1)
	(constituent verb finite main)
	(constituent rhema 2)
	(confidence 20)
	(check verb-finite))

; (could also be a direct or indirect object)
(df rhema
	(flags 1)
	(constituent object)
	(constituent rhema 2 finite)
	(seme (trajectory projective))
	(confidence 10)
	(check object))

;; Second position in the rhema
; (could be nothing at all ("ty vidíš")).
(df rhema
	(flags 2)
	(constituent nothing object))

; (could be a verb)
(df rhema
	(flags 2)
	(constituent verb finite main)
	(confidence 2)
	(check verb-finite))

; (could also be a direct or indirect object)
(df rhema
	(flags 2)
	(constituent object)
	(seme (trajectory projective))
	(confidence 7)
	(check object))

; Nothing
(df nothing (flags object) (theta (accusative patient)) (seme -patient))

;
(df slot
	(flags rhema)
	(constituent rhema 1))
