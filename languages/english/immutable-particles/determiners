(df determiner
	(seme -owner)
	(lit the) (space)
	(seme definite)
	(rection (definition definite)))

(df determiner
	(seme -owner)
	(fork
		((lit a) (space) (sandhi -vowel))
		((lit an) (space) (sandhi vowel)))
	(seme -definite (number singular))
	(rection (definition indefinite))
	(rection countable))

(df determiner
	(seme -owner)
	(rection countable)
	(seme -definite (number plural))
	(rection (definition indefinite)))
	
(df determiner
	(seme -owner)
	(rection -countable)
	(seme -definite (number singular))
	(rection (definition indefinite)))

(df determiner
	(lit my) (space)
	(into owner seme (head referential) (person first) (number singular)))

(df determiner
	(lit your) (space)
	(into owner seme (head referential) (person second) (number singular)))

(df determiner
	(lit his) (space)
	(into owner seme (head referential) (person third) (number singular) (gender male)))

(df determiner
	(lit her) (space)
	(into owner seme (head referential) (person third) (number singular) (gender female)))
