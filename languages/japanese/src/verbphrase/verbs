(df verbphrase
	(adjunct argument ha)
	(adjunct argument ga)
	(adjunct argument wo)
	(call verb-and-endings))

(df verb-and-endings
	(call verb ichidan)
	(call ending ichidan))

(df verb-and-endings
	(call verb suru)
	(call suru))

(df ending
	(flags ichidan)
	(lit る)
	(call hiragana)
	(seme (tense present)))

(df ending
	(flags ichidan)
	(lit た)
	(call hiragana)
	(seme (tense past)))

(df ending
	(flags ichidan)
	(lit ru)
	(call romaji)
	(seme (tense present)))

(df ending
	(flags ichidan)
	(lit ta)
	(call romaji)
	(seme (tense past)))
