
(df ko-so-a-do-prefix (call mora ko) (into spatiodeixis seme (d proximal)))
(df ko-so-a-do-prefix (call mora so) (into spatiodeixis seme (d medial)))
(df ko-so-a-do-prefix (call mora a)  (into spatiodeixis seme (d distal)))

(df ko-so-a-do
	(flags adnominal)
	(call ko-so-a-do-prefix)
	(call mora no)
	(call #))
	
(df ko-so-a-do
	(flags pronominal)
	(call ko-so-a-do-prefix)
	(call mora re)
	(call #)
	(seme (head referential) (person third)))
