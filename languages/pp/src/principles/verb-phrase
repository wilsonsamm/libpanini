(df verbphrase
	(call verb intransitive)
	(tag))

(for s (2 3 4 5)
(df verbphrase
	(language (head first) (stage .s))
	(call verb transitive)
	(into (theta object) call object)
	(tag))

(df verbphrase
	(language (head last) (stage .s))
	(into (theta object) call object)
	(call verb transitive)
	(tag)))
