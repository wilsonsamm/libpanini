(for theta (agent patient source)
(df verbstem
	(theta (subject .theta))
	(flags intransitive)
	(open verbstem
		(segments 10)
		(flags intransitive)
		(seme head)
		(theta subject))))

(for theta (agent patient source)
(df verbstem
	(theta (subject .theta))
	(flags transitive)
	(open verbstem
		(segments 10)
		(flags intransitive)
		(seme head)
		(theta subject))))

(df verb
	(tag)
	(flags intransitive finite)
	(call verbstem intransitive)
	(call conjugation)
	(segments space))
(df verb
	(tag)
	(flags transitive finite)
	(call verbstem transitive)
	(call conjugation)
	(segments space))

(for tense (past present future)
(df tam
	(tag)
	(flags .tense)
	(seme (tense .tense))
	(rection (tense .tense)))

(df conjugation
	(tag)
	(flags .tense)
	(call tam .tense)
	(into (theta subject) call conj-aff .tense))

(df conj-aff
	(flags .tense)
	(open conj-aff
		(flags .tense)
		(rection person number)
		(segments 4))))

