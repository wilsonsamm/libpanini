(df vb-conj
	(seme (time present))
	(into (theta subject) rection (person I)))
(df vb-conj
	(seme (time present))
	(into (theta subject) rection (person you)))
(df vb-conj
	(seme (time present))
	(into (theta subject) rection (person we)))
(df vb-conj
	(seme (time present))
	(into (theta subject) rection (person they)))
(df vb-conj
	(seme (time present))
	(into (theta subject) rection (person he/she/it))
	(lit s))
(df vb-conj
	(seme (time past))
	(lit d))

(df verb
	(flags intransitive finite)
	(constituent verbstem intransitive)
	(constituent vb-conj)
	(space))
	
(df verb
	(flags transitive finite)
	(constituent verbstem transitive)
	(constituent vb-conj)
	(space))
