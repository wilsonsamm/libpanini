
;;; Dd
(df verbstem
	(flags intransitive)
	(seme (head dead))
	(lit die)
	(theta (subject patient)))

;;; Ee
(df verbstem
	(flags intransitive)
	(seme (head exist))
	(lit exist)
	(theta (subject agent)))

;;; Ss
(df verbstem
	(flags intransitive)
	(seme -patient (head see))
	(theta (subject agent)) 
	(lit see))

(df verbstem
	(flags transitive)
	(seme (head see))
	(theta (subject agent) (object-direct patient))
	(lit see))
