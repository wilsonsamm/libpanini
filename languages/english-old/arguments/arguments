(df subject
	(into (theta subject) constituent pronoun subject))
	
(df subject
	(into (theta subject) constituent nounphrase))
	
(df object
	(flags direct)
	(into (theta object-direct) constituent pronoun object))

(df object
	(flags direct)
	(into (theta object-direct) constituent nounphrase))

(df object
	(flags indirect)
	(into (theta object-indirect) constituent pronoun object))

(df object
	(flags indirect)
	(into (theta object-indirect) constituent nounphrase))
