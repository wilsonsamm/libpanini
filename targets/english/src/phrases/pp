(df from-source
	(lit from) (segments space)
	(into source call object))

(df to-target
	(lit to) (segments space)
	(into target call object))

(df in-location
	(lit in) (segments space)
	(into location rection (location-preposition in))
	(into location call object))

(df to-location
	(lit to) (segments space)
	(into location rection (location-preposition to))
	(into location call pp-substantive))

(df prepositional-phrases
	(adjunct from-source)
	(adjunct to-target)
	(adjunct in-location)
	(adjunct to-location))
