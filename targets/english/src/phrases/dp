(df dp
	(flags unconjunctivised)
	(call determiner)
	(adjunct adjp attributive)
	(call noun)
	(adjunct prepositional-phrases)
	)

(df dp
	(flashbulb determiner)
	(seme (head union))
	(into 1 call dp unconjunctivised)
	(call conjunction)
	(into 2 call dp unconjunctivised)
	(rection (person they) (number plural))
	(kill-flashbulb determiner))

(df conjunction
	(lit and) (segments space)
	(seme (booles and)))
