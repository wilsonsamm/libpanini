(df complementiser
	(seme -exclamation -interrogative)
	(rection (complementiser none)))

(df complementiser
	(lit awa)
	(rection (complementiser awa)))

(df complementiser
	(lit yan)
	(seme -exclamation -interrogative (irrealis imperative))
	(rection (complementiser yan)))

(df complementiser
	(lit wa)
	(seme exclamation -interrogative)
	(rection (complementiser wa)))

(df complementiser-phrase
	(call clause finite)
	(call complementiser))
