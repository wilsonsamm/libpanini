; Each nounstem must have one of the flags masculine, feminine and neuter.
; Each nounstem must have one of the flags animate, -animate
; feminine and neuter nouns are always -animate.


(df nounstem
	(segments v e l r y b)
	(flags feminine -animate)
	(seme (head whale)))

(df nounstem
	(segments b y t ?e k)
	(flags masculine -animate)
	(seme (head house)))

(df nounstem
	(segments l e d ?e n)
	(flags masculine -animate)
	(seme (head january)))

(df nounstem
	(segments ú n o r)
	(flags masculine -animate)
	(seme (head february)))

(df nounstem
	(segments b ř e z e n)
	(flags masculine -animate)
	(seme (head march)))

(df nounstem
	(segments d u b ?e n)
	(flags masculine -animate)
	(seme (head april)))

(df nounstem
	(segments k v ě t ?e n)
	(flags masculine -animate)
	(seme (head may)))

(df nounstem
	(segments č e r v e n)
	(flags masculine -animate)
	(seme (head june)))

(df nounstem
	(segments č e r v e n ?e c)
	(flags masculine -animate)
	(seme (head july)))

(df nounstem
	(segments s r p ?e n)
	(flags masculine -animate)
	(seme (head august)))

(df nounstem
	(segments z á ř í)
	(flags masculine -animate)
	(seme (head september)))

(df nounstem
	(segments ř í j e n)
	(flags masculine -animate)
	(seme (head october)))

(df nounstem
	(segments l i s t o p a d)
	(flags masculine -animate)
	(seme (head november)))

(df nounstem
	(segments p r o s i n ?e c)
	(flags masculine -animate)
	(seme (head december)))

(df nounstem
	(segments č l o v ě k)
	(flags masculine animate)
	(seme (head man)))
