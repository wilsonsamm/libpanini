
(df num1
	(flags 0))

(df num1
	(flags k)
	(segments k ?i))

(df num2
	(flags 0)
	(call indicative)
	(call present)
	(into (theta subject) rection (number singular)))

(df num2
	(flags k)
	(call nonpast) (call optative)
	(into (theta subject) rection (number plural)))

(df num2
	(flags 0)
	(call indicative)
	(call present)
	(segments saltillo)
	(into (theta subject) rection (number plural)))

(for num1 (0 k)
 (df number
	(call num1 .num1)
